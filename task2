Console.Write("Введите первое число:");
int number = int.Parse(Console.ReadLine()!);
Console.Write("Введите второе число:");
int number2 = int.Parse(Console.ReadLine()!);
if (number2 > number)
{
    Console.WriteLine($"{number2} больше {number}");
}
if (number2 == number)
{
    Console.WriteLine($"{number2} равен {number}");
}
else Console.WriteLine($"{number} больше {number2}");
