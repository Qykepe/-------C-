Console.Write("Введите координаты первой точки:");
int ax = int.Parse(Console.ReadLine()!);
int ay = int.Parse(Console.ReadLine()!);
int ac = int.Parse(Console.ReadLine()!);

Console.Write("Введите координаты второй точки:");
int bx = int.Parse(Console.ReadLine()!);
int by = int.Parse(Console.ReadLine()!);
int bc = int.Parse(Console.ReadLine()!);

double ab = Math.Sqrt(Math.Pow((ax - bx), 2) + Math.Pow((ay - by), 2) + Math.Pow((ac - bc), 2));
Console.WriteLine($"{ab:f2}");